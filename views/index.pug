extends layout

block content
 div(class='main')
  head
    title Connect with buddies!
  body
    h2 Scan the QR code to begin team creation.
    a(href='http://localhost:8000/newteam')
      != new_team_qr_code
 nav(class='navbar navbar-inverse navbar-fixed-bottom')
  div(class='container-fluid')
    div(class='navbar-header')
     h2 Current ranking:
    ol
     each item in keys_sorted
      li= item